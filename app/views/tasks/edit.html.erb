<h1>Edit Task</h1>

<%= form_for(@task) do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>
  <br>
  <%= f.label :details %>
  <%= f.text_field :details %>
  <br>
  <%= f.label :complete %>
  <%= f.check_box :complete %>
  <br>
  <%= f.submit %>
  <br>


<% end %>
<%= link_to "Go Back", tasks_path%>

